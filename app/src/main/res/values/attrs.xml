<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!--APP标题栏扩展属性-->
    <declare-styleable name="titleBar">
        <!--是否显示返回按钮-->
        <attr name="isShowReturn" format="boolean" />
        <!--是否显示底部分割线-->
        <attr name="isShowDivide" format="boolean" />
        <!--页面标题-->
        <attr name="title" format="string" />
        <!--右边操作图标-->
        <attr name="rightImageRes" format="reference" />
        <!--右边操作文字-->
        <attr name="rightTxtString" format="string" />
        <!--右边操作文字颜色-->
        <attr name="rightTxtColor" format="color" />
    </declare-styleable>
</resources>
